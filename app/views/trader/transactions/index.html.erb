<h1>Trader::Transactions#index</h1>
<p>Find me in app/views/trader/transactions/index.html.erb</p>

<% if @user_transactions.any? %>
// list all transactions of trader
  <ul>
      <% @user_transactions.each do |transaction| %>
          <li>
              <%= link_to transaction.id, trader_transaction_path(transaction) %>
          </li>
      <% end %>
  </ul>
<% else %>
  <p>No transactions yet</p>
<% end %>