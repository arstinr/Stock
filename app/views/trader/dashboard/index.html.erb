<h1>Trader::Dashboard#index</h1>
<p>Find me in app/views/trader/dashboard/index.html.erb</p>

<%= link_to "Log out", destroy_user_session_path, data: { "turbo-method": :delete } %>

<p><%= @trader.email %></p>

<% if @trader.is_approved? %>
  <p>Your account has been approved!</p>
  <p>Display stocks partial here</p>
<% else %>
  <p>Your account is pending approval.</p>
<% end %>